{
  "name": "ardwiino",
  "version": "0.1.0-2",
  "license": "MIT",
  "description": "An application for configuring custom guitar hero guitars",
  "scripts": {
    "dev": "electron-webpack dev",
    "compile": "electron-webpack",
    "dist": "yarn compile && electron-builder && build --publish onTagOrDraft",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null",
    "postinstall": "electron-builder install-app-deps",
    "schemagen": "node schemaGen.js"
  },
  "dependencies": {
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "@types/react": "^16.8.16",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.0.9",
    "@types/react-router-dom": "^4.3.3",
    "@types/request-promise-native": "^1.0.16",
    "@types/serialport": "^7.0.3",
    "@types/tmp": "^0.1.0",
    "@types/usb": "^1.5.1",
    "axios": "^0.18.0",
    "c-struct": "^0.0.5",
    "delay": "^4.2.0",
    "electron-util": "^0.11.0",
    "node-intelhex": "^2.0.4",
    "node-pty": "^0.8.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-markdown": "^4.0.8",
    "react-redux": "^7.0.3",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.1",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "request-promise-native": "^1.0.7",
    "serialport": "^7.1.5",
    "source-map-support": "^0.5.10",
    "tmp": "^0.1.0",
    "typeface-roboto": "^0.0.54",
    "typescript-parser": "^2.6.1",
    "usb": "^1.5.0"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.0.0",
    "c-ast": "^1.0.0",
    "electron": "4.0.1",
    "electron-builder": "20.38.4",
    "electron-webpack": "^2.6.2",
    "electron-webpack-ts": "^3.1.1",
    "node-c-parser": "^1.0.11",
    "raw-loader": "^2.0.0",
    "stream-buffers": "^3.0.2",
    "ts-node": "^8.2.0",
    "typescript": "^3.4.5",
    "webpack": "4.28.4"
  },
  "electronWebpack": {
    "renderer": {
      "webpackConfig": "webpack.server.js"
    },
    "whiteListedModules": [
      "react-redux"
    ]
  },
  "author": {
    "name": "Sanjay Govind",
    "email": "sanjay.govind9@gmail.com",
    "url": "https://tangentmc.net"
  },
  "repository": "https://github.com/sanjay900/guitar-configurator",
  "build": {
    "extraResources": [
      {
        "from": "./binaries",
        "to": "binaries",
        "filter": [
          "**/*"
        ]
      }
    ],
    "asarUnpack": [
      "binaries/avrdude/**"
    ],
    "appId": "com.tangentmc.guitar-configurator",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "win": {
      "target": [
        "msi",
        "portable"
      ]
    },
    "files": [
      "dist"
    ],
    "linux": {
      "target": [
        "AppImage",
        "deb",
        "snap"
      ],
      "synopsis": "An application for configuring custom guitar hero guitars",
      "category": "Settings",
      "maintainer": "sanjay900 <sanjay.govind9@gmail.com>"
    },
    "directories": {
      "output": "release"
    }
  }
}
