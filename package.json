{
  "name": "guitar-configurator",
  "version": "1.0.0",
  "description": "An application for configuring custom guitar hero guitars",
  "main": "./dist/main.js",
  "scripts": {
    "build": "rimraf dist && tsc && npm run html",
    "watch": "tsc -w",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "start": "npm run build && electron ./dist/main.js",
    "install": "electron-rebuild",
    "pack": "electron-builder --dir",
    "dist": "npm run build && electron-builder -w",
    "html": "find . -type f \\( -iname '*.html' -o -iname '*.jpg' -o -iname '*.png' \\) -exec cp --parents {} ./dist \\;",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "appId": "com.tangentmc.guitar-configurator",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "win": {
      "target": "nsis"
    },
    "files": [
      "dist"
    ],
    "linux": {
      "target": [
        "AppImage"
      ],
      "icon": "asset/icons/app.linux.png",
      "synopsis": "For testing ng-electron-structure",
      "category": "Development"
    }
  },
  "repository": "https://github.com/electron/electron-quick-start-typescript",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo",
    "typescript"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^4.0.8",
    "electron-builder": "^20.39.0",
    "electron-rebuild": "^1.8.4",
    "rimraf": "^2.6.3",
    "tslint": "^5.10.0",
    "typescript": "^2.8.3"
  },
  "dependencies": {
    "@types/bootstrap": "^4.3.0",
    "@types/delay": "^3.1.0",
    "@types/jenkins": "^0.23.1",
    "@types/request-promise-native": "^1.0.15",
    "@types/serialport": "^7.0.2",
    "bootstrap": "^4.3.1",
    "chip.avr.avr109": "^1.1.1",
    "delay": "^4.1.0",
    "jenkins": "^0.23.0",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.7",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "serialport": "^7.1.4"
  }
}
